            <string>//Gravity
if (place_free(x, y+1))
{
    gravity = 3;
    //image_angle = direction;
}
else
{
    gravity = 0;
}


//Too fast
if (hspeed &gt; 0.5)
{
    hspeed = 0.5;
}
else if (hspeed &lt; -0.5)
{
    hspeed = -0.5;
}
//Push
if (distance_to_object(obj_pelaaja) &lt; 10 and obj_pelaaja.push == 1)
{
    if (instance_nearest(x, y, obj_pelaaja).x &lt; x
        and keyboard_check(vk_right))
    {
        hspeed = 0.7;
    }
    else if (instance_nearest(x, y, obj_pelaaja).x &gt; x
        and keyboard_check(vk_left))
    {
        hspeed = -0.7;
    }
    else if (instance_nearest(x, y, obj_pelaaja).x &gt; x
        and keyboard_check(vk_right))
    {
        hspeed = 0.3;
    }
    else if (instance_nearest(x, y, obj_pelaaja).x &lt; x
        and keyboard_check(vk_left))
    {
        hspeed = -0.3;
    }
    else if (!keyboard_check(vk_left) and !keyboard_check(vk_right))
    {
        hspeed = 0;
        vspeed = 0;
    }
}
else
{
    hspeed = 0;
    vspeed = 0;
}
</string>
