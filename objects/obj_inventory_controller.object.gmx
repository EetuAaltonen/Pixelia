            <string>//Collected item name
if (draw_name == true)
{
    if (text_y_pos == 0)
    {
        if not(name[t] == "")
        {
            draw_name = true;
            text_y_pos -= 1;
            alarm[1] = 2;
        }
        else
        {
            draw_name = false;
            t = 1;
        }
    }
    else
    {
        if not(text_y_pos == -40)
        {
            text_y_pos -= 1;
            alarm[1] = 2;
            if (text_y_pos &gt; -29 and name_alpha &lt; 1)
            {
                name_alpha += 0.10;   
            }
            else if (text_y_pos &lt; -29)
            {
                name_alpha -= 0.10;
            }
        }
        else
        {
            text_y_pos = 0;
            name_alpha = 0;
            name[t] = "";
            if (t + 1 &lt; array_length_1d(name))
            {
                t += 1;
            }
            else
            {
                if not(name[1] == "")
                {
                    t = 1;
                    draw_name = true;
                }
            }
            alarm[1] = 2;
        }
    } 
}
</string>
