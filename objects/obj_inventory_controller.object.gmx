            <kind>1</kind>
            <string>//Font
draw_set_font(fnt_item_counter);
draw_set_color(c_white);

//Coins
var coin;
coin = global.coins;
draw_text_color(view_xview+440,view_yview+278, string(coin),
                c_black, c_black, c_black, c_black, 1);
//Coin logo
draw_sprite_ext(spr_money_draw, image_index, view_xview+425, 
                view_yview+285, image_xscale, image_yscale, 
                image_angle, c_white, image_alpha);                
//Potions
var potion;
potion = global.potions;
draw_text_color(view_xview+380,view_yview+278, string(potion),
                c_black, c_black, c_black, c_black, 1);
                
//Potion logo
draw_sprite_ext(spr_potion, image_index, view_xview+372, 
                view_yview+285, image_xscale, image_yscale, 
                image_angle, c_white, image_alpha);

//Inventory
if (global.hud_state == "inventory1")
{
    if (loop_stop == false)
    {
        //Better code
        item_count = 0;
        search_id = "mater";
        
        ini_open("Inventory.sav");
        for(item_id = 1; item_id &lt; 101; item_id++)
        {
            key = search_id + "[" + string(item_id) + "]";
            //Item amount
            item_amount[item_id] = ini_read_real("Save1", key, -1);
            
            //Check if item amount &gt; 0
            if (round(item_amount[item_id]) &gt; 0)
            {
                //Item icon
                scr_inventory_item_info();
                item_icon[item_id] = string(Icon[item_id]);
                
                //Item description
                item_description[item_id] = string(Descr[item_id]);
                
                //Item amount is top of for loop
                
                //Draw
                item_icon_draw[item_id] = item_icon[item_id];
                item_description_draw[item_id] = item_description[item_id];
                item_amount_draw[item_id] = item_amount[item_id];
                
                item_count += 1;
            }
            //Change category
            if (search_id == "mater" and item_id == 100)
            {
                //search_id = "ingr";
                break;
            }
            /*else if(search_id == "ingr" and item_id &lt; 101)
            {
            }*/
            /*else
            {
                break;
            }*/
        }
        ini_close();
        loop_stop = true;
    }
    var y_pos; //y position
    y_pos = 110;
    if (item_count &gt; 0)
    {
        if (page_refresh == true)
        {
            size_of_list = 8; //Limit of list
            start_count = 0; //First item
            if (item_count &gt; 8)
            {                       //This have to be set!
                if ((item_count - ((inventory_page+1)*8)) &gt; 0 and inventory_page == 0)
                {
                    start_count = 0;
                    end_count = 8;
                    
                }
                else if ((item_count - ((inventory_page+1)*8)) &gt; 0 and
                        inventory_page &gt; 0)
                {
                    start_count = (inventory_page*8);
                    end_count = ((inventory_page*8)*2);
                }
                else
                {
                    start_count = (inventory_page*8);
                    end_count = item_count;
                }
            }
            else
            {
                end_count = item_count;
                start_count = 0;
            }
            page_update = false;
        }
        item_id = start_count;
        var i;
        for (i = start_count; i != end_count; i++;)
        {
            if (string_pos("spr_", string(item_icon_draw[item_id])))
            { 
                //Item background
                draw_sprite(spr_inv_item_bg, 0,view_xview+30, view_yview+7+y_pos);
                //Item icons
                draw_sprite(asset_get_index(string(item_icon_draw[item_id])), 0,view_xview+30, view_yview+7+y_pos);           
                //Item description background
                draw_sprite(spr_inv_item_description_bg, 0,view_xview+120, view_yview+7+y_pos);
                //Item description
                draw_text_color(view_xview+55,view_yview+y_pos, string(item_description_draw[item_id]),
                                c_black, c_black, c_black, c_black, 1);
                //Item amount background
                draw_sprite(spr_inv_item_amount_bg, 0,view_xview+228, view_yview+7+y_pos);
                //Item amount
                draw_text_color(view_xview+207,view_yview+y_pos, string(item_amount_draw[item_id]),
                                c_black, c_black, c_black, c_black, 1);
                y_pos += 19;
            }
            else
            {
                i -= 1;
            }
            item_id += 1;
        }
        //Page number background
        draw_sprite(spr_inv_item_description_bg, 0,view_xview+87, view_yview+95);
        //Page number max
        inventory_page_max = (round(item_count/8));
        if ((inventory_page_max - (item_count/8)) &lt; 0 )
        {   
            inventory_page_max += 1;
        }
        //Draw page number and max
        draw_text_color(view_xview+20,view_yview+87, 
                        string("PAGE: " + string(inventory_page + 1) + "       / " + string(inventory_page_max)),
                        c_black, c_black, c_black, c_black, 1);
    }
    else
    {
        //If empty
        draw_text_color(view_xview+30, view_yview+117, "Inventory is empty...",
                            c_black, c_black, c_black, c_black, 1);
    }
}
else if (global.hud_state == "inventory2")
