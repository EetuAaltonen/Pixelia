      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (!string_pos("inventory", string(global.hud_state)))
{
    //Use
    if (use == 0)
    {
        //Use
        use = 1;
        instance_create(x,y,obj_use);
        alarm[2] = 20;
    }

    //Push crate
    if (push == 1)
    {
        push = 0;
        sprite_index = spr_pelaaja;
    }
    
    else if (push == 0 and 
        distance_to_object(instance_nearest(x, y, obj_crate)) &lt; 5
        and place_meeting(x+3, y, instance_nearest(x, y, obj_crate)) ||
        place_meeting(x-3, y, instance_nearest(x, y, obj_crate)))
    {
        push = 1;
    }

    //Mining
    if (mining == 0 and distance_to_object(obj_iron_ore) &lt; 10)
    {
        mining = 1;
        alarm[8] = 50; //mining
    }
    else if (mining == 1)
    {
        mining = 0;
        sprite_index = spr_pelaaja;
    }

    //Woodcutting
    if (instance_exists(obj_tree))
    {
        if (woodcutting == 0 and distance_to_object(obj_tree) &lt; 10)
        {
            woodcutting = 1;
            alarm[9] = 50; //woodcutting
        }
        else if (woodcutting == 1)
        {
            woodcutting = 0;
            sprite_index = spr_pelaaja;
        }
    }
    
    //Ladder
    if (place_meeting(x, y, obj_ladder) and ladder == 0 and keyboard_check(vk_down))
    {
        ladder = 1;
        x = (instance_nearest(x, y, obj_ladder).x);
    }
    else if (place_meeting(x, y, obj_ladder) and ladder == 1)
    {
        ladder = 0;
    }
    
    //Cave Room Transport
    if (distance_to_object(obj_cave_door) &lt; 4)
    {
        room_goto(Cave);
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
