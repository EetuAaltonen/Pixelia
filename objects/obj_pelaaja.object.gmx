    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Push
if (push == 1)
{
    scr_player_push();
}

//Bow
else if (global.plr_bow == true)
{
    sprite_index = spr_player_bow;
    //Movement
    if (keyboard_check(vk_right))
    {
        hspeed = 1;
    }
    else if (keyboard_check(vk_left))
    {
        hspeed = -1;
    }
    //X_Scale
    if (obj_plr_aim.x &gt; x)//(hspeed &gt; 0) 
    {
        image_xscale = 1;
    }

    else if (obj_plr_aim.x &lt; x)//(hspeed &lt; 0) 
    {
        image_xscale = -1;
    }
    //Stop
    if (keyboard_check_released(vk_right) || keyboard_check_released(vk_left))
    {
        hspeed = 0;
    }
}

//If some menu is open
else if not(global.hud_state == "null")
{
    hspeed = 0;
    vspeed = 0;
}

//Shop
else if (global.shop &gt; 0) //Same as previous?
{
    hspeed = 0;
    vspeed = 0;
}

//Dialog
else if (global.dialog == true)//Same as 2x previous?
{
    hspeed = 0;
    vspeed = 0;
}

//Mining
else if (mining == 1)
{
    hspeed = 0;
    vspeed = 0;
    sprite_index = spr_pelaaja_mining;
    image_speed = 0.1;
    if (obj_iron_ore.x &gt; x)
    {
        image_xscale = 1;
    }
    else if (obj_iron_ore.x &lt; x)
    {
        image_xscale = -1;
    }
    if (obj_iron_ore.iron == 1)
    {
        mining = 0;
        sprite_index = spr_pelaaja;
    }
}

//Woodcutting
else if (woodcutting == 1)
{
    scr_player_woodcutting();
}

//Ladder
else if (place_meeting(x, y, obj_ladder) and ladder == 1)
{
    if (ladder == 1)
    {
        x = (instance_nearest(x, y, obj_ladder).x);
    }
    if (keyboard_check(vk_up) and ladder == 1)
    {
        vspeed = -2;
        x = (instance_nearest(x, y, obj_ladder).x)   
    }
    else if (keyboard_check(vk_down) and ladder == 1)
    {
        vspeed = 2;
        x = (instance_nearest(x, y, obj_ladder).x) 
    }
    else if (ladder == 1 and !keyboard_check(vk_up) and !keyboard_check(vk_down))
    {
        vspeed = 0;
        gravity = 0;
    }
}
else
{
    //Gravity
    if (place_free(x, y+3) and roll == 0)
    {
        gravity = 0.3;
    }
    else if (!place_free(x, y+3) and roll == 0)
    {
        gravity = 0.5;
    }
    
    //Too fast falling
    if (vspeed &lt; -4)
    {
        vspeed = -4;
    }
    
    //Ladder...
    ladder = 0;

    //X_Scale
    if (hspeed &gt; 0) 
    {
        image_xscale = 1;
    }

    else if (hspeed &lt; 0) 
    {
        image_xscale = -1;
    }

    //Movement
    if (keyboard_check(vk_right))
    {
        hspeed = 2;
    }
    else if (keyboard_check(vk_left))
    {
        hspeed = -2;
    }
    
    //Run   
    if (keyboard_check(vk_shift) 
        and keyboard_check(vk_left)
        and not keyboard_check(vk_down) 
        and !global.stamina == 0
        and !place_free(x, y+3))
    {
        image_speed = 0.5;
        hspeed = -4;
    }
    
    else if (keyboard_check(vk_shift)
        and keyboard_check(vk_right)
        and not keyboard_check(vk_down)
        and !global.stamina == 0
        and !place_free(x, y+3))
    {
        image_speed = 0.5;
        hspeed = 4;
    }
    
    //Stop
    if (keyboard_check_released(vk_right) || keyboard_check_released(vk_left))
    {
        hspeed = 0;
    }

    //Jump
    if (keyboard_check(vk_up) and not place_free(x, y+1) and
        ladder == 0 and can_jump == 1)
    {
        can_jump = 0;
        alarm[10] = 10; //can_jump
        vspeed -= 4;
        
        vspeed = -4;
        sprite_index = spr_pelaaja_jump;
        // audio_play_sound(snd_jump,1,false);
    }
    else if (keyboard_check_released(vk_up))
    {
        sprite_index = spr_pelaaja;
    }
    
    //Roll
    if (roll == 1 and  hspeed &gt; 0 and not keyboard_check(vk_shift)
        and !place_free(x, y+3))
    {
        global.damage = 0;
        sprite_index = spr_pelaaja_roll;
        hspeed = 5;
        gravity = 0;
        image_speed = 1;
        image_xscale = 1;
        image_alpha = 0.5;
    }
    else if (roll == 1 and hspeed &lt; 0 and not keyboard_check(vk_shift)
             and !place_free(x, y+3))
    {
        global.damage = 0;
        sprite_index = spr_pelaaja_roll;
        hspeed = -5;
        gravity = 0;
        image_speed = 1;
        image_xscale = -1;
        image_alpha = 0.5;
    }
    
    //Down
    if (keyboard_check(vk_down) and hspeed == 0
        and !keyboard_check(vk_left) 
        and !keyboard_check(vk_right))
    {
        image_speed = 0;
        sprite_index = spr_pelaaja_down;
    }
    else if (keyboard_check(vk_down) and keyboard_check(vk_left))
    {
        image_speed = 0.2;
        hspeed = -1;
        sprite_index = spr_pelaaja_down;
    }
    else if (keyboard_check(vk_down) and keyboard_check(vk_right))
    {
        image_speed = 0.2;
        hspeed = 1;
        sprite_index = spr_pelaaja_down;
    }
    else if (keyboard_check_released(vk_down))
    {
        image_speed = 0;
        sprite_index = spr_pelaaja; 
    }
    
    //Blocked
    if (!place_free(x,y-3))
    {
        image_speed = 0;
        sprite_index = spr_pelaaja_down;
        alarm[7] = 10; //Fix animation stuck
    }
    
    //Stairs
    scr_player_cave_stairs();
}
</string>
