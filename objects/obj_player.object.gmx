    <event eventtype="10" enumb="69">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (!string_pos("inventory", string(global.hud_state)))
{
    //Push crate
    if (action_state == "push")
    {
        action_state = "null";
        sprite_index = spr_player;
    }
    
    else if (action_state == "null" and 
        distance_to_object(instance_nearest(x, y, obj_crate)) &lt; 5
        and place_meeting(x+3, y, instance_nearest(x, y, obj_crate)) ||
        place_meeting(x-3, y, instance_nearest(x, y, obj_crate)))
    {
        action_state = "push";
    }

    //Mining
    if (action_state == "null" and distance_to_object(obj_iron_ore) &lt; 10)
    {
        action_state = "mining";
        alarm[8] = 50;
    }
    else if (action_state == "mining")
    {
        action_state = "null";
        sprite_index = spr_player;
    }

    //Woodcutting
    if (instance_exists(obj_tree))
    {
        if (action_state == "null" and distance_to_object(obj_tree) &lt; 10)
        {
            action_state = "woodcutting"
            alarm[9] = 50;
        }
        else if (action_state == "woodcutting")
        {
            action_state = "null";
            sprite_index = spr_player;
        }
    }
    
    //Fishing
    if (action_state == "null" and distance_to_object(obj_fishing_spot) &lt; 4)
    {
        obj_player.action_state = "fishing";
        scr_plr_fishing();
        instance_create(obj_player.x, obj_player.y-10, obj_fishing_bobber);
        instance_create(obj_player.x, obj_player.y, obj_fishing_line);
    }
    else if (action_state == "fishing")
    {
        action_state = "null";
        sprite_index = spr_player;
    }
    
    //Ladder
    if (place_meeting(x, y, obj_ladder) and
        action_state == "null" and keyboard_check(vk_down))
    {
        action_state = "ladder";
        x = (instance_nearest(x, y, obj_ladder).x);
    }
    else if (place_meeting(x, y, obj_ladder) and action_state == "ladder")
    {
        action_state = "null";
    }
    
    //Cave Room Transport
    if (distance_to_object(obj_cave_door) &lt; 4)
    {
        room_goto(Cave);
    }
}
</string>
          </argument>
        </arguments>
      </action>
