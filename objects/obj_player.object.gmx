    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>obj_player</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Action states
//If some menu is open
if not(global.hud_state == "null")
{
    hspeed = 0;
    vspeed = 0;
}
//Push
else if (action_state == "confirm")
{
    hspeed = 0;
    vspeed = 0;
}
//Push
else if (action_state == "push")
{
    scr_plr_push();
}
//Ladder
else if (action_state == "ladder")
{
    if (distance_to_object(obj_ladder) &lt; 2)
    {
        scr_plr_ladder();
    }
    else
    {
        action_state = "null";
    }
}
//Bow
else if (action_state == "bow")
{
    scr_plr_bow();
}
//Mining
else if (action_state == "mining")
{
    scr_plr_mining();
}
//Woodcutting
else if (action_state == "woodcutting")
{
    scr_plr_woodcutting();
}
//Fishing
else if (action_state == "fishing")
{
    scr_plr_fishing();
}
//Planting seeds
else if (action_state == "planting")
{
    hspeed = 0;
    vspeed = 0;
}

//Default movements
else if (action_state == "null")
{
    //Gravity
    if (place_free(x, y+3) and roll == 0)
    {
        gravity = 0.3;
    }
    else if (!place_free(x, y+3) and roll == 0)
    {
        gravity = 0.5;
    }
    
    //Too fast falling
    if (vspeed &lt; -4)
    {
        vspeed = -4;
    }

    //X_Scale
    if (hspeed &gt; 0) 
    {
        image_xscale = 1;
    }

    else if (hspeed &lt; 0) 
    {
        image_xscale = -1;
    }

    //Movement
    if (keyboard_check(vk_right))
    {
        hspeed = 2;
    }
    else if (keyboard_check(vk_left))
    {
        hspeed = -2;
    }
    
    //Run   
    if (keyboard_check(vk_shift) 
        and keyboard_check(vk_left)
        and not keyboard_check(vk_down) 
        and !global.stamina == 0
        and !place_free(x, y+3))
    {
        image_speed = 0.5;
        hspeed = -4;
    }
    
    else if (keyboard_check(vk_shift)
        and keyboard_check(vk_right)
        and not keyboard_check(vk_down)
        and !global.stamina == 0
        and !place_free(x, y+3))
    {
        image_speed = 0.5;
        hspeed = 4;
    }
    
    //Stop
    if (keyboard_check_released(vk_right) || keyboard_check_released(vk_left))
    {
        hspeed = 0;
    }

    //Jump
    if (keyboard_check(vk_up) and not place_free(x, y+1))
    {
        can_jump = 0;
        alarm[10] = 10; //can_jump
        vspeed -= 4;
        
        vspeed = -4;
        sprite_index = spr_plr_jump;
        // audio_play_sound(snd_jump,1,false);
    }
    else if (keyboard_check_released(vk_up))
    {
        sprite_index = spr_player;
    }
    
    //Roll
    if (roll == 1 and  hspeed &gt; 0 and not keyboard_check(vk_shift)
        and !place_free(x, y+3))
    {
        global.damage = 0;
        sprite_index = spr_plr_roll;
        hspeed = 5;
        gravity = 0;
        image_speed = 1;
        image_xscale = 1;
        image_alpha = 0.5;
    }
    else if (roll == 1 and hspeed &lt; 0 and not keyboard_check(vk_shift)
             and !place_free(x, y+3))
    {
        global.damage = 0;
        sprite_index = spr_plr_roll;
        hspeed = -5;
        gravity = 0;
        image_speed = 1;
        image_xscale = -1;
        image_alpha = 0.5;
    }
    
    //Down
    if (keyboard_check(vk_down) and hspeed == 0
        and !keyboard_check(vk_left) 
        and !keyboard_check(vk_right))
    {
        image_speed = 0;
        sprite_index = spr_plr_down;
    }
    else if (keyboard_check(vk_down) and keyboard_check(vk_left))
    {
        image_speed = 0.2;
        hspeed = -1;
        sprite_index = spr_plr_down;
    }
    else if (keyboard_check(vk_down) and keyboard_check(vk_right))
    {
        image_speed = 0.2;
        hspeed = 1;
        sprite_index = spr_plr_down;
    }
    else if (keyboard_check_released(vk_down))
    {
        image_speed = 0;
        sprite_index = spr_player; 
    }
    
    //Stairs
    if (place_meeting(x+hspeed, y, obj_stairs_0001))
    {
        scr_plr_cave_stairs();
    }
    if (distance_to_object(obj_stairs_0001) == 0)
    {
        scr_solidify();
        scr_falling_damage();
    }
    
    //Blocked
    if (!place_free(x,y-3))
    {
        image_speed = 0;
        sprite_index = spr_plr_down;
        alarm[7] = 10; //Fix animation stuck
    }
}
</string>
          </argument>
        </arguments>
