          <argument>
            <kind>1</kind>
            <string>if (instance_exists(obj_pelaaja) and global.plr_bow == true)
{   
    dir_now := point_direction(obj_pelaaja.x,obj_pelaaja.y,x,y);
    dir_new := (dir_now + aim_speed + 360)mod(360); //360
    x := lengthdir_x(50,dir_new) + obj_pelaaja.x;
    y := lengthdir_y(50,dir_new) + obj_pelaaja.y;
    hspeed = obj_pelaaja.hspeed;
    vspeed = obj_pelaaja.vspeed;
    if ((keyboard_check(vk_up) and !keyboard_check(vk_down) and
        (obj_pelaaja.image_xscale = 1 and (x &gt; (obj_pelaaja.x + 5)))) or
        (keyboard_check(vk_up) and !keyboard_check(vk_down) and
        (obj_pelaaja.image_xscale = 1 and  y &gt; obj_pelaaja.y)))
    {
        if (keyboard_check(vk_shift))
        {
            aim_speed = 2;
        }
        else
        {
            aim_speed = 1;    
        }
    }
    else if ((keyboard_check(vk_up) and !keyboard_check(vk_down) and
        (obj_pelaaja.image_xscale = -1 and (x &lt; (obj_pelaaja.x - 5)))) or
        (keyboard_check(vk_up) and !keyboard_check(vk_down) and
        (obj_pelaaja.image_xscale = -1 and y &gt; obj_pelaaja.y)))
    {
        if (keyboard_check(vk_shift))
        {
            aim_speed = -2;
        }
        else
        {
            aim_speed = -1;    
        }
    }
    else if ((keyboard_check(vk_down) and !keyboard_check(vk_up) and
        (obj_pelaaja.image_xscale = 1 and (x &gt; (obj_pelaaja.x + 5)))) or
        (keyboard_check(vk_down) and !keyboard_check(vk_up) and
        (obj_pelaaja.image_xscale = 1 and y &lt; obj_pelaaja.y)))
    {
        if (keyboard_check(vk_shift))
        {
            aim_speed = -2;
        }
        else
        {
            aim_speed = -1;    
        }
    }
    else if ((keyboard_check(vk_down) and !keyboard_check(vk_up) and
        (obj_pelaaja.image_xscale = -1 and (x &lt; (obj_pelaaja.x - 5)))) or
        (keyboard_check(vk_down) and !keyboard_check(vk_up) and
        (obj_pelaaja.image_xscale = -1 and y &lt; obj_pelaaja.y)))
    {
        if (keyboard_check(vk_shift))
        {
            aim_speed = 2;
        }
        else
        {
            aim_speed = 1;    
        }
    }
    else
    {
        aim_speed = 0;
    }
}
else
{
    global.plr_bow = false;
    instance_destroy();
}
</string>
